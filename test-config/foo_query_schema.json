{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "size": {
      "type": "number",
      "description": "Number of records to fetch",
      "minimum": 10,
      "maximum": 1000
    },
    "start": {
      "type": "number",
      "description": "Current start number",
      "minimum": 0,
      "maximum": 5
    }
  },
  "required": ["size", "start"]
}
